{
  "version": 2,
  "builds": [
    { "src": "api/server.js", "use": "@vercel/node" },
    { "src": "*.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/.*", "dest": "api/server.js" },
    { "src": "/static/(.*)", "dest": "client/static/$1" },
    { "src": "/(.*)", "dest": "client/index.html" }
  ]
}
